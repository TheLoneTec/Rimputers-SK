<?xml version="1.0" encoding="utf-8" ?>
<Patch>	

<Operation Class="PatchOperationFindMod">
    <mods>
		<li>Medieval Overhaul</li>
    </mods>
	<match Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName = "RP_MechanicalCalculator"]/costList</xpath>
				<value>
					<costList>
					<DankPyon_ComponentBasic>10</DankPyon_ComponentBasic>
					</costList>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName = "RP_DifferenceEngine"]/costList</xpath>
				<value>
					<costList>
					<DankPyon_ComponentBasic>100</DankPyon_ComponentBasic>
					</costList>
				</value>
			</li>
		</operations>
	</match>
</Operation>

</Patch>